<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.examentap.controllers.propiedadSeleccionada"
      prefHeight="400.0" prefWidth="600.0"
      alignment="CENTER" spacing="20">
    <padding>
        <Insets bottom="20.0" top="20.0" left="20.0"/>
    </padding>

    <TextArea fx:id="ta_All_Propiedades2" prefWidth="250" prefHeight="300" maxHeight="300" maxWidth="250" editable="false"/>
    <ImageView fx:id="iv_imagen2" fitWidth="200" fitHeight="200" preserveRatio="true" />
    <Button fx:id="cerrarButton" layoutX="200.0" layoutY="320.0" text="Cerrar" onAction="#onClosePropiedadView"/>

</VBox>
